{
  "name": "thekonz/lighthouse-redis-broadcaster",
  "description": "A lighthouse-php redis broadcaster to be used with laravel-echo-server.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    { 
      "name": "Konstantin Duczek",
      "email": "konz.duczek@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "nuwave/lighthouse": "^4",
    "illuminate/support": "^7.7",
    "illuminate/console": "^7.7",
    "illuminate/cache": "^7.7",
    "illuminate/redis": "^7.7",
    "illuminate/broadcasting": "^7.7",
    "ext-json": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1"
  },
  "autoload": {
    "psr-4": {
      "thekonz\\LighthouseRedisBroadcaster\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "thekonz\\LighthouseRedisBroadcaster\\SubscriptionServiceProvider"
      ]
    }
  }
}